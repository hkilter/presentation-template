% Title page
\defbeamertemplate*{title page}{texsx}[1][]
{ 
   \vbox{}
   \vfill
   \vskip4cm%
    \begin{beamercolorbox}[wd=\the\paperwidth,center,sep=0pt,#1]{title page header}
      \usebeamerfont{title}\inserttitle\par%
    \end{beamercolorbox}%
    \vskip0.75cm%
    \begin{beamercolorbox}[wd=\the\paperwidth,center,#1]{author}
      \usebeamerfont{author}\insertauthor%
    \end{beamercolorbox}
     \vskip0.2cm%
    \begin{beamercolorbox}[wd=\the\paperwidth,center,#1]{date}
      \usebeamerfont{author}\insertdate%
    \end{beamercolorbox}
   \vfill
}

